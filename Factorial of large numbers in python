# Factorial of large numbers in python


def factorial(n):
#for negative value
    if n<0: 
        return None  
    result=1  #for factorial of 1
#start from value or number2
    for i in range(2,n+1): 
        result *= i
    return result
num = 20
print("The factorial is:",factorial(num))


# time complexity is O(n)
# space complexity is O(1)
