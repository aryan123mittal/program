# Rotate Array by One in python 


def ByOne(arr):
        n = len(arr)
        for i in range(n):
            temp = arr[i]
            arr[i] = arr[n-1]
            arr[n-1] = temp
        return arr
arr = [1, 2, 3, 4, 5]
rotate = ByOne(arr)
print(rotate)


# time complexity O(n)
# space complexity O(1)
